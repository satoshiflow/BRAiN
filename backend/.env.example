# ============================================
# BRAiN Backend Configuration (Example)
# ============================================

# API Settings
API_TITLE=BRAIN API
API_VERSION=2.0.0
API_PREFIX=/api
DEBUG=false

# Database
DATABASE_URL=postgresql://brain:changeme123@postgres:5432/brain
REDIS_URL=redis://redis:6379/0
QDRANT_URL=http://qdrant:6333

# Anthropic API Keys (Multi-Key Setup)
ANTHROPIC_API_KEY=sk-ant-api03-xxxxx
ANTHROPIC_API_KEY_BRAIN=sk-ant-api03-xxxxx
ANTHROPIC_API_KEY_ODOO=sk-ant-api03-xxxxx
ANTHROPIC_API_KEY_000_APPS=sk-ant-api03-xxxxx
ANTHROPIC_API_KEY_001_LANDRAD=sk-ant-api03-xxxxx
ANTHROPIC_API_KEY_002_FEWO=sk-ant-api03-xxxxx

# Token Limits (daily)
ANTHROPIC_DAILY_LIMIT_BRAIN=1000000
ANTHROPIC_DAILY_LIMIT_ODOO=500000
ANTHROPIC_DAILY_LIMIT_000_APPS=200000

# Moonshot AI (Kimi) - For Aider Development
# Get your key from: https://platform.moonshot.cn/console/api-keys
# Used by scripts/aider-kimi.sh for local development
MOONSHOT_API_KEY=sk-your-moonshot-key-here

# Agent Settings
MAX_CONCURRENT_AGENTS=10
AGENT_TIMEOUT=300

# Logging
LOG_LEVEL=INFO
